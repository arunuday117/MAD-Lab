<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ETA_HDS" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_pale</item>
        <item name="colorPrimaryVariant">@color/blue_pale</item>
        <item name="colorOnPrimary">@color/white_200</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#F493B6</item>
        <item name="colorSecondaryVariant">#CBCFF5</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/blue_pale</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.splashScreenT" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_image</item>
        <item name="android:statusBarColor">@color/white</item>
    </style>
</resources>